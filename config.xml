<?xml version='1.0' encoding='utf-8'?>
<widget id="com.klokantech.osmvectoroffline" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>OSM-Vector-Offline</name>
    <description>
        OpenStreetMap vector tiles with native MapBox GL and Apache Cordova / PhoneGap Build.
    </description>
    <author email="info@klokantech.com" href="http://klokantech.com">
        Klokan Technologies GmbH Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-mapbox" spec="1">
        <param name="ACCESS_TOKEN" value="pk.eyJ1Ijoib3NtMnZlY3RvcnRpbGVzIiwiYSI6ImNpajJuNmY1MDAwMGF2bmx6bTNnZmthOW0ifQ.rkzJYaSkeK1F3g9EcBCh6g" />
    </plugin>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
